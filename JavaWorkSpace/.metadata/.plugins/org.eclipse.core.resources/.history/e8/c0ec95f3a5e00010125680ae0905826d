package JavaStart;

import java.util.Iterator;

public class lesson1 {
	
	public static char grade(int score) {
char ret;
		
		if (score >= 80) {
			return 'D';
		}else if (score >= 50) {
			return 'P';
		}else {
			return 'F';
		}
		
		return ret;
	}
	}
}

	public static void main(String[] args) {
		
}
