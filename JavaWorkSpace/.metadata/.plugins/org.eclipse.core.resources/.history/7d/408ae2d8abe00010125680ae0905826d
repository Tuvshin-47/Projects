package JavaStart;

public class Ticket {
    
    public int ticketPrice(int age, boolean member) {
        if ((age <= 10) || (age >= 60)) {
            return 10;
        } else {
            return 20;
        }
        // Note: The 'member' parameter is not used in this logic
        // If you want to use it, you could add additional logic like:
        // if (member) { return price - 5; } // discount for members
    }
    
    public static void main(String[] args) {
        Ticket ticket = new Ticket();
        System.out.println("Price for age 8: " + ticket.ticketPrice(8, false));
        System.out.println("Price for age 65: " + ticket.ticketPrice(65, true));
        System.out.println("Price for age 30: " + ticket.ticketPrice(30, false));
    }
}